
CC = @CC@

CFLAGS =@oshwcflags@

OBJS = sdloshw.o sdltimer.o sdlerr.o sdlin.o sdlout.o sdlres.o sdltext.o \
       sdlerr.o

liboshw.a: $(OBJS)
	ar crs $@ $^
	cp liboshw.a ..

sdlerr.o  : sdlerr.c sdlgen.h ../defs.h ../gen.h ../oshw.h
sdlin.o   : sdlin.c sdlgen.h ../defs.h ../gen.h ../oshw.h
sdloshw.o : sdloshw.c sdlgen.h ../defs.h ../gen.h ../oshw.h ../err.h
sdlout.o  : sdlout.c sdlgen.h sdlres.h sdltext.h ccfont.c ../defs.h ../gen.h \
            ../oshw.h ../err.h ../state.h
sdlres.o  : sdlres.c sdlres.h sdlgen.h ccicon.c ../defs.h ../gen.h ../oshw.h \
            ../err.h ../state.h
sdltext.o : sdltext.c sdltext.h sdlgen.h ../defs.h ../gen.h ../oshw.h ../err.h
sdltimer.o: sdltimer.c sdlgen.h ../defs.h ../gen.h ../oshw.h


clean:
	rm -f $(OBJS) liboshw.a ../liboshw.a
