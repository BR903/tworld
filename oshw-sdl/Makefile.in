
CC = @CC@

CFLAGS =@oshwcflags@

OBJS = sdloshw.o sdltimer.o sdlerr.o sdlin.o sdlout.o sdlres.o sdltext.o \
       sdltile.o sdlerr.o

liboshw.a: $(OBJS)
	ar crs $@ $^
	cp liboshw.a ..

sdlerr.o  : sdlerr.c sdlgen.h ../gen.h ../oshw.h
sdloshw.o : sdloshw.c sdlgen.h ../gen.h ../oshw.h ../err.h ../defs.h
sdltimer.o: sdltimer.c sdlgen.h ../gen.h ../oshw.h ../defs.h
sdltext.o : sdltext.c sdlgen.h ../gen.h ../oshw.h ../err.h
sdltile.o : sdltile.c sdlgen.h ../gen.h ../oshw.h ../err.h \
            ../defs.h ../state.h
sdlres.o  : sdlres.c sdlgen.h ccicon.c ../gen.h ../oshw.h ../err.h
sdlin.o   : sdlin.c sdlgen.h ../gen.h ../oshw.h ../defs.h
sdlout.o  : sdlout.c sdlgen.h ccfont.c ../gen.h ../oshw.h ../err.h \
            ../defs.h ../state.h


clean:
	rm -f $(OBJS) liboshw.a ../liboshw.a
